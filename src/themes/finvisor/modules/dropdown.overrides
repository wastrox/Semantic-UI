/*******************************
         Theme Overrides
*******************************/


.ui.fluid.dropdown{
  display: flex;
  align-items:center;
  justify-content: initial;
  max-width: 300px;
  font-size:1em;
  font-weight: initial;
  padding:8px;
  min-height: 32px;
}
.ui.fluid.dropdown > .dropdown.icon{
  padding:0;
  right: 5px;
  margin: 0;
  top: 50%;
  transform: translateY(-50%);
  color: @inputPlaceholderColor;
}
